        % if default_grade == grade:
        <li class="ui-selected" value="{{default_grade}}"><a href="#">{{default_grade}}</a></li>
        % else:
        <li value="{{grade}}"><a href="#">{{grade}}</a></li>
        % end
        % end
      </ul>
    </form>
    </div>
  <div id="gradingnav">
    <ul class="nav">
      % if prevstudent != None:
      <a id="prevstudentbutton" 
         href="{{studentbutton}}/{{prevstudent}}">previous student</a>
      % else:
      <span class="notalink">previous student</span>
      %end
      % if nextstudent != None:
      <a id="nextstudentbutton" 
         href="{{studentbutton}}/{{nextstudent}}">next student</a>
      % else:
      <span class="notalink">next student</span>
      % end
    </ul>
