<script type="text/javascript"> 
     $(function() {
    	var comments = {{past_comments}};	 
        $( "#comment" ).autocomplete({
	   source: comments
        });
        $( "#searchbutton").button({
        icons: {
           secondary:"ui-icon-search"
          }
        });
        $( "#prevstudentbutton").button({
        icons: {
          primary:"ui-icon-arrowthick-1-w"
        }
       });       
        $( "#nextstudentbutton").button({
        icons: {
          secondary:"ui-icon-arrowthick-1-e"
        }
       });        
        
        $( ":button, input:submit").button();
        $( ":button, input:submit").button();
        $( "#help" ).accordion({
               collapsible: true,
               active: false,
               icons: { 'header': 'ui-icon-help', 'headerSelected': 'ui-icon-minus' }

         });
       $( "#btnHide").button().click(function() {
             $('#help').hide('slide');
             $('#btnHide').hide();
             $.cookie('hideHelp', 'true');
          });
     });
</script>

